{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "eqeqeq": [2, "smart"],
    "max-depth": [1, 3],
    "max-statements": [1, 20],
    "no-console": 1,
    "no-extend-native": 2,
    "no-unused-vars": 1
  }
}
