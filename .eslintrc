{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "eqeqeq": [2, "smart"],
    "max-depth": [1, 3],
    "max-statements": [1, 15],
    "no-extend-native": 2,
    "no-unused-vars": 1
  }
}
